<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="OutlandBuildingBase">
		<defName>Outland_VathunReliefA</defName>
		<label>vathun stone relief (a)</label>
		<description>A huge stone carved for decoration.</description>
		<graphicData>
			<texPath>Outland/Vathun/Buildings/FloorDeco_A</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3, 3)</drawSize>
		</graphicData>
		<size>(3, 3)</size>
		<uiIconScale>1.0</uiIconScale>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<fillPercent>0.1</fillPercent>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<pathCost>30</pathCost>
		<passability>Standable</passability>
		<rotatable>true</rotatable>
		<canOverlapZones>true</canOverlapZones>
		<tradeability>Sellable</tradeability>
		<tickerType>Normal</tickerType>
		<statBases>
			<MarketValue>4000</MarketValue>
			<MaxHitPoints>250</MaxHitPoints>
			<Flammability>0</Flammability>
			<Beauty>0</Beauty>
			<WorkToBuild>8000</WorkToBuild>
		</statBases>
		<costStuffCount>100</costStuffCount>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<designationCategory>Outland_Outland</designationCategory>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>Outland_SubCatDecoration</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

